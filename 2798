N,M = map(int,input().split())
cards = list(map(int,input().split()))


cards.sort()
sum = 0

for i in range(N):
    for j in range(i+1,N):
        for k in range(j+1,N):
            cur_sum = cards[i] + cards[j] + cards[k]
            if (sum < cur_sum <= M ):
                sum = cur_sum

print(sum)
