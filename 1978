N = int(input())
nums = list(map(int,input().split()))

count = 0
for i in nums:
    if i == 1 :
        continue

    is_prime = True  ##Flag 활용하기 (소수라 가정하고 추후 내리기)

    for j in range(2,int(i**0.5) + 1):
        if(i % j == 0):
            is_prime = False
            break
    
    if is_prime:
        count += 1

print(count)
